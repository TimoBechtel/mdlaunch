{
  "name": "mdlaunch",
  "version": "0.0.2",
  "description": "Quickly spin up a HTML presentation from any existing markdown file.",
  "bin": "lib/main.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "prepare": "npm run build",
    "build": "rollup -c",
